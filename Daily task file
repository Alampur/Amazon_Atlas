$today = Get-Date
$year = $today.Year
$month = $today.Month
[System.Globalization.CultureInfo]::CurrentCulture.Calendar.GetDaysInMonth($year, $month) | ForEach-Object {
    "{0}/{1}/{2}" -f $month, $_, $year
}
